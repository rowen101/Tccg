    </div>

    <div class="control-group">
        @Html.LabelFor(model => model.ISCUSTOMER, new { @class = "control-label" })

        <div class="controls">
            @Html.EditorFor(model => model.ISCUSTOMER)
        </div>
    </div>

    <div class="control-group">
        <div class="controls">

            <p>
                <input type="submit" class="btn btn-primary" value="Create" />
                @Html.ActionLink("Back to List", "client-inquiry")</p>
        </div>
    </div>
}




@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
